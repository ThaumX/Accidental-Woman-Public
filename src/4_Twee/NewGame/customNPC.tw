  <<set _c = 2>>
  <<for _i = 0; _i < _list[_c].length; _i++>>
  <<set _pri = '<<radiogaga "setup.npcSetting.persCore[' + _c +'][' + _i + ']" setup.npcSetting.persCore[' + _c +'][' + _i + '] "Much Less" "Less" "Neutral" "More" "Much More">>'>>
  <tr><td><<print _list[_c][_i]>></td><td><<print _pri>></td></tr>
