  <<set _list = [false,["strength","secure","preg","kids","evolve","pleasure"],["strength","life","liberty","property","honesty","integrity"],["strength","interest","empathy","caring","trust","altruism"],["strength","thoughtful","responsible","attention","trustworthy","structure"],["strength","betrayal","cheating","effort","permanence","family"],["strength","complex","learning","abstract","curiosity","novelty"],["strength","impulsive","unstable","addiction","anxiety","sensitive","anger","sadness"],["strength","selfinterest","selfworth","confidence","fragility","selfimage","mach","risk"]]>>
