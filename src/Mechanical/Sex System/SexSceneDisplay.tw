:: SexSceneDisplay [nobr sex]

<<silently>>
<<procS "display load">>
<<set _pcOrgPer = Math.round(($PC.status.pleasure/$sex.pcOrgasm)*100)>>
<<if _pcOrgPer > 100>><<set _pcOrgPer = 100>><<set _pcOrgText = "Cumming!">><<else>><<set _pcOrgText = _pcOrgPer + "%">><</if>>
<<set _printerA = "<div class='sex-stat-bar'><span class='stat-bar-rating' role='stat-bar' style='width: " + _pcOrgPer + "%; background: hotpink;'><span id='bitch'>" + _pcOrgText + "</span></span></div>">>
<<if $sex.target == "none">>
  <<set _npcStatBar = 0>>
<<else>>
  <<set _npcStatBar = $sex.target>>
<</if>>
<<set _npcOrgPer = Math.round(($NPC[$sex.activeNPC[_npcStatBar]].status.pleasure / $sex.npcOrgasm[_npcStatBar])*100)>>
<<if _npcOrgPer > 100>><<set _npcOrgPer = 100>><<set _npcOrgText = "Cumming!">><<else>><<set _npcOrgText = _npcOrgPer + "%">><</if>>
<<set _printerB = "<div class='sex-stat-bar'><span class='stat-bar-rating' role='stat-bar' style='width: " + _npcOrgPer + "%; background: hotpink;'><span id='bitch'>" + _npcOrgText + "</span></span></div>">>
<<set _npcNamePrint = $NPC[$sex.activeNPC[_npcStatBar]].main.name + " @@.red;♥@@">>
<</silently>>
<div id="sexStats">
<table id="invisTable">
<tr><td style="width:10%">
@@.white;.quest;.px16;Oh!-METER@@
</td><td style="width:5%">
@@.white;.quest;.px16;PC@@
</td><td style="width:35%">
<<print _printerA>>
</td><td style="width:10%">
@@.white;.quest;.px16;<span id="npcstatbarlabel"><<print _npcNamePrint>></span>@@
</td><td style="width:35%">
<span id="npcstatbar"><<print _printerB>></span>
</td><td>
<<link [img[Next NPC|IMGswitchright]]>>
  <<if _npcStatBar < ($sex.npcCount - 1)>>
    <<set _npcStatBar += 1>>
  <<else>>
    <<set _npcStatBar = 0>>
  <</if>>
  <<if _npcStatBar == $sex.target>>
    <<set _npcNamePrint = $NPC[$sex.activeNPC[_npcStatBar]].main.name + " @@.red;♥@@">>
  <<else>>
    <<set _npcNamePrint = $NPC[$sex.activeNPC[_npcStatBar]].main.name>>
  <</if>>
  <<set _npcOrgPer = Math.round(($NPC[$sex.activeNPC[_npcStatBar]].status.pleasure / $sex.npcOrgasm[_npcStatBar])*100)>>
  <<if _npcOrgPer > 100>><<set _npcOrgPer = 100>><<set _npcOrgText = "Cumming!">><<else>><<set _npcOrgText = _npcOrgPer + "%">><</if>>
  <<set _printerB = "<div class='sex-stat-bar'><span class='stat-bar-rating' role='stat-bar' style='width: " + _npcOrgPer + "%; background: hotpink;'><span id='bitch'>" + _npcOrgText + "</span></span></div>">>
  <<replace "#npcstatbarlabel">><<print _npcNamePrint>><</replace>>
  <<replace "#npcstatbar">><<print _printerB>><</replace>>
<</link>>
</td></tr>
</table>
</div>
<div id="mainshitsSex">
<<if $sex.pcOutput != "none">>
  <<set _pcOutA = $sex.pcOutput.slice(0,1)>>
  <<set _pcOutB = $sex.pcOutput.slice(1)>>
  @@.head3;<<print _pcOutA>>@@<<print _pcOutB>>
  <br><br>
<<else>>
  @@.head3;N@@o PC text output for some reason...<br><br>
<</if>>
<<set _textC = 0>>
<<if $sex.npcOutput.constructor === Array>>
  <<for _i = 0, _c = $sex.npcOutput.length; _i < _c; _i++>>
    <<if $sex.npcOutput[_i] != "none">>
      <<set _npcOutA = $sex.npcOutput[_i].slice(0,1)>>
      <<set _npcOutB = $sex.npcOutput[_i].slice(1)>>
      <<set _textC += 1>>
      @@.head3;<<print _npcOutA>>@@<<print _npcOutB>>
      <br><br>
    <</if>>
  <</for>>
  <<if _textC == 0>>
    @@.head3;N@@o NPC text output for some reason...
  <</if>>
<<elseif "string" == typeof $sex.npcOutput && $sex.npcOutput != "none">>
  <<set _npcOutA = $sex.npcOutput.slice(0,1)>>
  <<set _npcOutB = $sex.npcOutput.slice(1)>>
  @@.head3;<<print _npcOutA>>@@<<print _npcOutB>>
<</if>>
<table id="invisTable">
<tr><td colspan=3 class="ident">Debug Info</td></tr>
<tr><td>Position: <<print setup.sexPos[$sex.pos].longName>></td>
<td>Connected: <<print setup.sexPos[$sex.pos].prime.connected>></td>
<td>ConPoints: <<print setup.sexPos[$sex.pos].prime.pcConPoint>>,<<print setup.sexPos[$sex.pos].prime.npcConPoint>></td></tr>
<tr><td>PC Facing: <<print setup.sexPos[$sex.pos].prime.pcFacing>></td>
<td>NPC Facing: <<print setup.sexPos[$sex.pos].prime.npcFacing>></td>
<td>Last Act: <<print $sex.pcAct>></td></tr></table>

</div>
<<if $sex.orgCountPC < 1>>
  <div id="actionSex">
  <table>
  <tr><td colspan=2><table id="invisTable"><tr>
  <td id="actionSexTabs"><<include [[SexSceneDisplayTabs]]>></td><td style="width:160px"><<include [[sexSceneQuickButtons]]>></td>
  </tr></table>
  </td></tr><tr><td style="width:1px"><br><br><br><br><br></td><td id="sexActButtons">
  <<include [[sexSceneDisplayButts]]>>
  </td></tr><tr><td colspan=2 id="sexActHovertext">
  <span id="targetname" class="ship"><<if $sex.target != "none">>Target: <<print $NPC[$sex.activeNPC[$sex.target]].main.name>> <<print $NPC[$sex.activeNPC[$sex.target]].main.surname.slice(0,1)>>. <</if>></span><<insertion sexacttab1>>Do something involving your hands.<<endinsertion>><<insertion sexacttab2>>Do something involving your mouth.<<endinsertion>><<insertion sexacttab3>>Say something, or at least make some sort of noise.<<endinsertion>><<insertion sexacttab4>>Change your movement or change position.<<endinsertion>><<insertion sexacttab5>>Do something kinky!<<endinsertion>><<insertion sexacttab6>>Do something that doesn't fit into the other categories.<<endinsertion>><<insertion sexacttab9>>Change the NPC you are targeting with your action (if relevant). Not all actions require a target.<<endinsertion>><<insertion sexacttab7>>Repeat your previous action.<<endinsertion>><<insertion sexacttab8>>Go with the flow. Don't do anything special, and just keep going with whatever you're doing.<<endinsertion>><<insertion actmenulicksomething>>Lick part of--or a place on--your target's body.<<endinsertion>>
  <<set _positions = Object.keys(setup.sexPos)>>
  <<set _posLength = _positions.length>>
  <<for _i = 0; _i < _posLength; _i++>>
    <<print setup.sexPos[_positions[_i]].hovtext>>
  <</for>>
  <<set _acts = Object.keys(setup.sexActs)>>
  <<set _actLength = _acts.length>>
  <<for _i = 0; _i < _actLength; _i++>>
    <<print setup.sexActs[_acts[_i]].hovtext>>
  <</for>>
  </td></tr>
  </table>
  </div>
<<else>>
<<button "Continue">><<goto [[sexSceneControl]]>><</button>>
<</if>>
<<procE>>
<<timed 3s>>
<<replace "#right-UI-proc-disp">>
<<print $proc.text>>: <<print $proc.time>>ms
<</replace>>
<</timed>>


