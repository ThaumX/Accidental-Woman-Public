:: sexSceneDisplayButts [nobr]


<<if $sex.tabs == 4>>
<<button "< Slow Down">><</button>><<silently>>
<<switch $sex.speed>>
<<case 0>>
  <<set _speedout = "N/A">>
<<case 1>>
  <<set _speedout = "Not Moving">>
<<case 2>>
  <<set _speedout = "Grinding">>
<<case 3>>
  <<set _speedout = "Slow">>
<<case 4>>
  <<set _speedout = "Leisurely">>
<<case 5>>
  <<set _speedout = "Average">>
<<case 6>>
  <<set _speedout = "Quick">>
<<case 7>>
  <<set _speedout = "Intense">>
<<case 8>>
  <<set _speedout = "Hard Pounding">>
