{
    "defaultSeverity": "warning",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
      "object-literal-sort-keys": false,
      "max-classes-per-file": false,
      "max-line-length": [false, {
        "limit": 240,
        "ignore-pattern": "// |`[^`]*`"
      }],
      "no-consecutive-blank-lines": false,
      "only-arrow-functions": false,
      "no-console": false,
      "no-eval": false,
      "prefer-for-of": false,
      "variable-name": false,
      "class-name": false,
      "interface-name": false
    },
    "rulesDirectory": []
}